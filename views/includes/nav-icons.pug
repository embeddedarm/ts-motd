div.nav-icons-container
  div.nav-icons
    unless current == 'home'
      a.no-color(href="/").nav-button
        span#home-button.fa-stack.fa-2x
          i.fa.fa-circle.fa-stack-2x
          i.fa.fa-pencil.fa-stack-1x.fa-inverse
    unless current == 'calendar'
      a.no-color(href="/calendar").nav-button
        span#calendar-button.fa-stack.fa-2x
          i.fa.fa-circle.fa-stack-2x
          i.fa.fa-calendar.fa-stack-1x.fa-inverse
    unless current == 'analytics'
      a.no-color(href="/analytics").nav-button
        span#analytics-button.fa-stack.fa-2x
          i.fa.fa-circle.fa-stack-2x
          i.fa.fa-bar-chart.fa-stack-1x.fa-inverse
